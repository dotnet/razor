      <trans-unit id="Setting_LogLevelNone">
        <source>None</source>
        <target state="translated">Нет</target>
        <note />
      </trans-unit>
      <trans-unit id="Setting_LogLevelTrace">
        <source>Trace</source>
        <target state="translated">Трассировка</target>
        <note />
      </trans-unit>
      <trans-unit id="Setting_LogLevelWarning">
        <source>Warning</source>
        <target state="translated">Предупреждение</target>
        <note />
      </trans-unit>
