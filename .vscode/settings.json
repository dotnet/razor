{
  "files.trimTrailingWhitespace": true,
  "files.trimTrailingWhitespaceInRegexAndStrings": false,
  "files.associations": {
    "*.*proj": "xml",
    "*.props": "xml",
    "*.targets": "xml",
    "*.tasks": "xml"
  },
  "[yaml]": {
    "editor.tabSize": 2
  },
  "[json]": {
    "editor.tabSize": 2
  },
  "dotnet.defaultSolution": "Razor.slnx",
  "dotnet.solution.autoOpen": "Razor.slnx",
  "files.encoding": "utf8bom",
  "dotnet.testWindow.disableBuildOnRefresh": true
}
